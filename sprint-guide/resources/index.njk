<h1>Resources</h1>
<article class="measure">
  {% renderFile './sprint-guide/resources/_introduction.md' %}
</article>

<h2>Software & Tools</h2>
<ul role="list">
  {% for item in software %}
  <li class="software">
    <img class="software__logo" src="{{ item.icon }}" alt="Icon for {{ item.title }}" />
    <div>
      <h3>{{ item.title }}</h3>
      <p>
        <small>Tags</small>
        {% for tag in item.tags %}<span class="tag">{{ tag }}</span>{% endfor %}
      </p>
      <p>{{ item.description }}</p>
    </div>
  </li>
  {% endfor %}
</ul>

<h2>Other Resources</h2>
{% renderFile './sprint-guide/resources/_list.md' %}
